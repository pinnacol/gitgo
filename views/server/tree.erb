<%= erb :_commit, :locals => {:commit => commit }, :layout => false %>

<%= erb :_treenav, :locals => {:id => id, :path => path}, :layout => false %>

<div class="tree">
  <ul class="contents"><% tree.contents.each do |obj| %><%         if obj.kind_of?(Grit::Tree) %>
    <li class="tree"><%= tree_link(id, path, obj.name) %></li><%   else %>
    <li class="blob"><%= blob_link(id, path, obj.name) %></li><%   end %><% end %>
  </ul>
</div>